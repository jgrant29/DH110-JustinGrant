            <g id="Group-5-Copy-2">
                <g id="Group">
                    <g id="Group-Copy-2">
                        <mask id="mask-14" fill="white">
                            <use xlink:href="#path-13"></use>
                        </mask>
                        <use id="☁️-Elevation" fill="#FFFFFF" xlink:href="#path-13"></use>
                        <g id="search" mask="url(#mask-14)" stroke="#FFFFFF">
                            <g transform="translate(242.475000, 23.578947)">
                                <ellipse id="Oval" stroke-width="0.75" cx="6.459375" cy="6.38596491" rx="6.084375" ry="6.01096491"></ellipse>
                                <line x1="12.8819444" y1="11.7530864" x2="15.6791667" y2="14.5185185" id="Line-4" stroke-width="1.25" stroke-linecap="round"></line>
                            </g>
                        </g>
                        <g id="Group" mask="url(#mask-14)">
                            <g transform="translate(278.000000, 20.000000)" id="📍-Leading-Icon-l-Use-High-Emphasis">
                                <mask id="mask-16" fill="white">
                                    <use xlink:href="#path-15"></use>
                                </mask>
                                <use id="-↳Color" fill="#FFFFFF" xlink:href="#path-15"></use>
                            </g>
                        </g>
                        <text id="✏️-Page-Title" mask="url(#mask-14)" font-family="Helvetica" font-size="20" font-weight="normal" line-spacing="24" letter-spacing="0.150000006">
                            <tspan x="18.88125" y="39" fill="#A62424">HR</tspan>
                            <tspan x="48.0679688" y="39" fill="#000000">S</tspan>
                        </text>
                        <line x1="0.496875" y1="55.5087719" x2="318.496875" y2="55.5087719" id="Line" stroke="#FFFFFF" stroke-linecap="square" mask="url(#mask-14)"></line>
                    </g>
